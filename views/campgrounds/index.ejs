<% layout('layouts/boilerplate') -%> 
    <h1>Campgrounds</h1>
    <a href="/campgrounds/new">Add New Campground</a>
    <ul>
        <% for(let camp of campgrounds) { %>
            <li><b><a href="/campgrounds/<%= camp._id %>"><%= camp.title %></a></b> - <%= camp.location %></li>
        <% } %>
    </ul>
